{
  "name": "eslint-plugin-rn-a11y",
  "version": "0.0.1",
  "description": "ESLint Plugin for Accessibility on React Native",
  "main": "./dist/index.js",
  "repository": "https://github.com/grgr-dkrk/eslint-plugin-rn-a11y",
  "license": "MIT",
  "author": {
    "name": "grgr-dkrk",
    "url": "https://github.com/grgr-dkrk"
  },
  "keywords": [
    "accessibility",
    "eslint",
    "react-native"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc -p ./tsconfig.build.json --outDir ./dist",
    "typeCheck": "tsc --noEmit",
    "lint": "eslint ./src/",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --detectOpenHandles --coverage tests/**/*",
    "format": "prettier --write ./src/",
    "ci": "yarn typeCheck && yarn lint && yarn test"
  },
  "dependencies": {
    "jsx-ast-utils": "^3.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.19",
    "@types/react-native": "^0.63.42",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "eslint": ">= 6"
  },
  "release": {
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/commit-analyzer",
      "@semantic-release/git",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator"
    ],
    "branches": [
      "main"
    ]
  }
}
