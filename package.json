{
  "name": "eslint-plugin-rn-a11y",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": "https://github.com/grgr-dkrk/eslint-plugin-rn-a11y",
  "license": "MIT",
  "author": {
    "name": "grgr-dkrk",
    "url": "https://github.com/grgr-dkrk"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json --outDir dist",
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "jest --coverage tests/**/*",
    "lint": "eslint ./src",
    "typeCheck": "tsc -p . --noEmit",
    "format": "prettier --write ./src",
    "ci": "yarn typeCheck && yarn lint && yarn test"
  },
  "dependencies": {
    "jsx-ast-utils": "^3.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
